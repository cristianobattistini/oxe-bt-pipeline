<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="put_down_blue_can">
      <SubTree ID="T_Navigate" target="table" name="subtree_00" />
      <Fallback name="place_phase">
        <SubTree ID="T_Manipulate_Place" target="table" obj="blue_can" name="subtree_01" />
        <Sequence name="recovery_place">
          <SubTree ID="T_Manipulate_Grasp" target="blue_can" name="subtree_02" />
          <SubTree ID="T_Navigate" target="table" name="subtree_03" />
          <SubTree ID="T_Manipulate_Place" target="table" obj="blue_can" name="subtree_04" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
  <BehaviorTree ID="T_Navigate">
    <RetryUntilSuccessful num_attempts="3" name="retryuntilsuccessful_00">
      <Action ID="NAVIGATE_TO" obj="{target}" name="navigate_to_00" />
    </RetryUntilSuccessful>
  </BehaviorTree>
  <BehaviorTree ID="T_Manipulate_Grasp">
    <RetryUntilSuccessful num_attempts="3" name="retryuntilsuccessful_01">
      <Action ID="GRASP" obj="{target}" name="grasp_00" />
    </RetryUntilSuccessful>
  </BehaviorTree>
  <BehaviorTree ID="T_Manipulate_Place">
    <Sequence name="sequence_00">
      <RetryUntilSuccessful num_attempts="3" name="retryuntilsuccessful_02">
        <Action ID="PLACE_ON_TOP" obj="{obj}" name="place_on_top_00" />
      </RetryUntilSuccessful>
      <RetryUntilSuccessful num_attempts="3" name="retryuntilsuccessful_03">
        <Action ID="RELEASE" name="release_00" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="white_carton" timeout_ms="1500"/>
    <OpenGripper width="0.06" timeout_ms="1000"/>
    <ApproachAndAlign target="white_carton" tolerance="0.01" timeout_ms="2500"/>
    <MoveAbove target="white_carton" offset_z="0.05" timeout_ms="1500"/>
    <ComputeGraspPose target="white_carton" strategy="top" result_key="goal_pose"/>
    <GraspAtPose pose_key="goal_pose" strategy="top" timeout_ms="2000"/>
    <CloseGripper force="20" timeout_ms="1000"/>
    <ObjectInGripper target="white_carton"/>
    <RaiseHeld target="white_carton" distance="0.10" timeout_ms="1500"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="brown_object" timeout_ms="2000"/>
    <OpenGripper width="0.08" timeout_ms="1000"/>
    <MoveAbove target="brown_object" offset_z="0.05" timeout_ms="2000"/>
    <ApproachAndAlign target="brown_object" tolerance="0.01" timeout_ms="3000"/>
    <IsObjectVisible target="brown_object"/>
    <ComputeGraspPose target="brown_object" strategy="top" result_key="std_top"/>
    <Pick grasp_key="std_top" timeout_ms="1500"/>
    <IsGraspStable/>
    <ObjectInGripper target="brown_object"/>
    <RaiseHeld target="brown_object" distance="0.10" timeout_ms="1000"/>
    <HoldObject target="brown_object" duration_ms="1000"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

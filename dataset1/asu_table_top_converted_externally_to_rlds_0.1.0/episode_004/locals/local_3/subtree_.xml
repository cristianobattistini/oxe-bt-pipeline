<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="brown_object" timeout_ms="1500"/>
    <ComputeGraspPose target="brown_object" strategy="top" result_key="goal_pose"/>
    <ApproachAndAlign target="brown_object" tolerance="0.01" timeout_ms="1500"/>
    <GraspAtPose pose_key="goal_pose" strategy="pinch" timeout_ms="2000"/>
    <CloseGripper force="20" timeout_ms="1000"/>
    <IsGraspStable/>
  </Sequence>
</BehaviorTree>
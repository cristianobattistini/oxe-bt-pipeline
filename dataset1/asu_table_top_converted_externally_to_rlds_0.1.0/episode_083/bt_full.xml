<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="3">
      <Action ID="DetectObject" target="brown_object" timeout_ms="2000"/>
    </RetryUntilSuccessful>
    <Condition ID="IsObjectVisible" target="brown_object"/>
    <Action ID="OpenGripper" width="0.08" timeout_ms="1000"/>
    <Action ID="ApproachAndAlign" target="brown_object" tolerance="0.005" timeout_ms="3000"/>
    <Action ID="Pick" grasp_key="std_side" timeout_ms="2000"/>
    <Condition ID="ObjectInGripper" target="brown_object"/>
    <Action ID="RaiseHeld" target="brown_object" distance="0.10" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <IsObjectVisible target="maroon_square"/>
    <RetryUntilSuccessful num_attempts="3">
      <Sequence>
        <ApproachAndAlign target="maroon_square" tolerance="0.01" timeout_ms="3000"/>
        <SetTCPYaw yaw_deg="0"/>
        <IsObjectVisible target="maroon_square"/>
        <Timeout timeout_ms="1500">
          <Push target="maroon_square" distance="0.15" direction_deg="0" timeout_ms="1500"/>
        </Timeout>
      </Sequence>
    </RetryUntilSuccessful>
    <IsObjectVisible target="maroon_square"/>
  </Sequence>
</BehaviorTree>
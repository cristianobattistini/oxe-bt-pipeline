<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="blue_can" timeout_ms="2000"/>
    <MoveAbove target="blue_can" offset_z="0.08" timeout_ms="3000"/>
    <ApproachAndAlign target="blue_can" tolerance="0.01" timeout_ms="3000"/>
    <OpenGripper width="0.06" timeout_ms="1000"/>
    <ComputeGraspPose target="blue_can" strategy="side" result_key="goal_pose"/>
    <GraspAtPose pose_key="goal_pose" strategy="side" timeout_ms="2000"/>
    <CloseGripper force="20" timeout_ms="1500"/>
    <RaiseHeld target="blue_can" distance="0.10" timeout_ms="1500"/>
    <ObjectInGripper target="blue_can"/>
  </Sequence>
</BehaviorTree>

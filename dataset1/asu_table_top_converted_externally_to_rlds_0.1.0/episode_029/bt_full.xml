<BehaviorTree ID="MainTree">
  <Sequence>
    <Condition ID="ObjectInGripper" target="green_glass_bottle"/>
    <Action ID="MoveAbove" target="place_on_table" offset_z="0.10" timeout_ms="2000"/>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.10" force_threshold="5" timeout_ms="2000"/>
    <Action ID="OpenGripper" width="0.08" timeout_ms="500"/>
    <Action ID="Retreat" distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

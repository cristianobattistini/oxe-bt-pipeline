<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="put_down_white_carton">
      <!-- Approach white carton -->
      <Action ID="NAVIGATE_TO" obj="white_carton"/>
      
      <!-- Grasp with fallback recovery -->
      <Fallback name="grasp_phase">
        <Action ID="GRASP" obj="white_carton"/>
        <Sequence name="recovery_grasp">
          <Action ID="NAVIGATE_TO" obj="white_carton"/>
          <Action ID="GRASP" obj="white_carton"/>
        </Sequence>
      </Fallback>
      
      <!-- Put down (release) the white carton -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>
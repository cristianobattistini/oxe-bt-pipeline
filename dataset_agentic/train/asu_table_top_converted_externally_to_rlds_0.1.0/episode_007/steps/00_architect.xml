<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="raise_white_carton">
      <!-- Approach -->
      <Action ID="NAVIGATE_TO" obj="white_carton"/>
      
      <!-- Grasp with recovery -->
      <Fallback name="grasp_phase">
        <Action ID="GRASP" obj="white_carton"/>
        <Sequence name="recovery_grasp">
          <Action ID="NAVIGATE_TO" obj="white_carton"/>
          <Action ID="GRASP" obj="white_carton"/>
        </Sequence>
      </Fallback>
      <!-- The robot is now holding (raising) the white carton -->
    </Sequence>
  </BehaviorTree>
</root>
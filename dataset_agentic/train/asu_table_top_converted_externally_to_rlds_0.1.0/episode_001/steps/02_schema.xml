<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="pick_green_bottle">
      <!-- Approach -->
      <Action ID="NAVIGATE_TO" obj="green_bottle"/>
      
      <!-- Grasp with recovery -->
      <Fallback name="grasp_phase">
        <Action ID="GRASP" obj="green_bottle"/>
        <Sequence name="recovery_grasp">
          <Action ID="NAVIGATE_TO" obj="green_bottle"/>
          <Action ID="GRASP" obj="green_bottle"/>
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
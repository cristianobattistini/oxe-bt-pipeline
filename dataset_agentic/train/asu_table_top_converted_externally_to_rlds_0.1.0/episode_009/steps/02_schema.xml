<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="pick_up_brown_object">
      <!-- Approach -->
      <Action ID="NAVIGATE_TO" obj="{target}"/>
      
      <!-- Grasp with recovery -->
      <Fallback name="grasp_phase">
        <Action ID="GRASP" obj="{target}"/>
        <Sequence name="recovery_grasp">
          <Action ID="NAVIGATE_TO" obj="{target}"/>
          <Action ID="GRASP" obj="{target}"/>
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
  <!-- Example of calling this tree as a SubTree -->
  <!--
  <BehaviorTree ID="PickUpBrownObject">
    <SubTree ID="MainTree" target="brown_object"/>
  </BehaviorTree>
  -->
</root>
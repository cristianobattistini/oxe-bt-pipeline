<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="grid_clamp" timeout_ms="1500"/>
    <ComputeGraspPose target="grid_clamp" strategy="top" result_key="goal_pose"/>
    <MoveAbove target="grid_clamp" offset_z="0.05" timeout_ms="1500"/>
    <ApproachAndAlign target="grid_clamp" tolerance="0.005" timeout_ms="1500"/>
    <PlaceAt pose_key="goal_pose" yaw_deg="0" press_force="10" timeout_ms="2000"/>
    <OpenGripper width="0.08" timeout_ms="1000"/>
    <Retreat distance="0.10" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence name="root_sequence">
    <Action ID="MoveAbove" name="MoveAbove_grasp" target="grid_clamp_source" offset_z="0.10" timeout_ms="2000"/>
    <Action ID="LowerUntilContact" name="Lower_to_grasp" speed="slow" max_depth="0.10" force_threshold="5" timeout_ms="1500"/>
    <Action ID="CloseGripper" name="Grasp_clamp" force="30" timeout_ms="800"/>
    <Condition ID="ObjectInGripper" name="Check_grasp" target="grid_clamp"/>
    <Action ID="RaiseHeld" name="Lift_clamp" target="grid_clamp" distance="0.10" timeout_ms="1500"/>
    <Action ID="MoveAbove" name="MoveAbove_place" target="grid_clamp_destination" offset_z="0.10" timeout_ms="2500"/>
    <Action ID="LowerUntilContact" name="Lower_to_place" speed="slow" max_depth="0.10" force_threshold="5" timeout_ms="1500"/>
    <Action ID="OpenGripper" name="Release_clamp" width="0.08" timeout_ms="800"/>
    <Action ID="Retreat" name="Retreat_arm" distance="0.15" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>
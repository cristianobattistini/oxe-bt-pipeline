<BehaviorTree ID="MainTree">
  <Sequence>
    <ObjectInGripper target="clamp"/>
    <DetectObject target="grid_clamp" timeout_ms="1500"/>
    <MoveAbove target="grid_clamp" offset_z="0.05" timeout_ms="2000"/>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="grid_clamp" tolerance="0.005" timeout_ms="3000"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="5" timeout_ms="2000"/>
    <PlaceAtTol pose_key="goal_pose" yaw_deg="90" press_force="10" tolerance="0.005" timeout_ms="2000"/>
    <OpenGripper width="0.06" timeout_ms="400"/>
    <Retreat distance="0.10" timeout_ms="800"/>
    <Wait timeout_ms="500"/>
  </Sequence>
</BehaviorTree>

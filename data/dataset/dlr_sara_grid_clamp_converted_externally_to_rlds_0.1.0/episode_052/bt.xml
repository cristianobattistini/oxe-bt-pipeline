<BehaviorTree ID="MainTree">
  <Sequence>
    <Action ID="ComputeGraspPose" target="grid_clamp" strategy="top" result_key="std_top"/>
    <Action ID="Pick" grasp_key="std_top" timeout_ms="4000"/>
    <Action ID="MoveAbove" target="fixture_id" offset_z="0.10" timeout_ms="3000"/>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.10" force_threshold="10" timeout_ms="2000"/>
    <Action ID="OpenGripper" width="0.08" timeout_ms="1000"/>
    <Action ID="Retreat" distance="0.15" timeout_ms="2000"/>
  </Sequence>
</BehaviorTree>
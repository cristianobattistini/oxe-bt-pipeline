<BehaviorTree ID="MainTree">
  <Sequence>
    <Action ID="DetectObject" target="towel" timeout_ms="800"/>
    <Action ID="ComputeGraspPose" target="towel" strategy="pinch" result_key="grasp_towel"/>
    <Action ID="GraspAtPose" pose_key="grasp_towel" strategy="pinch" timeout_ms="1000"/>
    <Action ID="Unfold" target="towel" strategy="pull" num_folds="1" timeout_ms="1500"/>
    <Action ID="PlaceDown" target="towel" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <Condition ID="IsAt" target="goal"/>
      <RetryUntilSuccessful num_attempts="3">
        <Sequence>
          <Action ID="NavigateToNamed" location_id="goal" yaw_deg="0" timeout_ms="7000"/>
          <Action ID="Wait" timeout_ms="500"/>
        </Sequence>
      </RetryUntilSuccessful>
    </Fallback>
    <Condition ID="IsAt" target="goal"/>
  </Sequence>
</BehaviorTree>

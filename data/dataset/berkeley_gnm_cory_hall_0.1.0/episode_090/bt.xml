<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsAt target="goal"/>
      <RetryUntilSuccessful num_attempts="3">
        <NavigateToNamed location_id="goal" yaw_deg="0" timeout_ms="3000"/>
      </RetryUntilSuccessful>
    </Fallback>
    <Wait timeout_ms="500"/>
    <IsAt target="goal"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="3">
      <Action ID="DetectObject" target="goal_marker" timeout_ms="2000"/>
    </RetryUntilSuccessful>
    <Timeout timeout_ms="7000">
      <Action ID="NavigateToNamed" location_id="goal" yaw_deg="0" timeout_ms="7000"/>
    </Timeout>
    <Condition ID="IsAt" target="goal"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <Condition ID="IsObjectVisible" target="peg"/>
      <RetryUntilSuccessful num_attempts="3">
        <Action ID="ScanForTarget" target="peg" pattern="raster" max_attempts="3" timeout_ms="800"/>
      </RetryUntilSuccessful>
    </Fallback>
    <Fallback>
      <Condition ID="IsObjectVisible" target="cup"/>
      <RetryUntilSuccessful num_attempts="3">
        <Action ID="ScanForTarget" target="cup" pattern="raster" max_attempts="3" timeout_ms="800"/>
      </RetryUntilSuccessful>
    </Fallback>
    <Action ID="DetectObject" target="peg" timeout_ms="800"/>
    <Action ID="DetectObject" target="cup" timeout_ms="800"/>
    <Action ID="MoveAbove" target="peg" offset_z="0.05" timeout_ms="1500"/>
    <Action ID="ApproachAndAlign" target="peg" tolerance="0.005" timeout_ms="1500"/>
    <Action ID="Grasp" target="peg" strategy="top" force="5" timeout_ms="800"/>
    <Condition ID="IsGraspStable"/>
    <Action ID="MoveAbove" target="cup" offset_z="0.05" timeout_ms="1500"/>
    <Action ID="ApproachAndAlign" target="cup" tolerance="0.005" timeout_ms="1500"/>
    <Action ID="InsertInto" target="peg" slot_id="receptacle_A" axis="z" distance="0.05" tolerance="0.005" force_limit="5" timeout_ms="1000"/>
    <Condition ID="Inserted" target="peg" slot_id="receptacle_A"/>
    <Action ID="OpenGripper" width="0.06" timeout_ms="500"/>
    <Action ID="Retreat" distance="0.05" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>
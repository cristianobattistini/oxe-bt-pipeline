<BehaviorTree ID="MainTree">
  <Sequence>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="1200"/>
    <Condition ID="ContactDetected" force_threshold="10"/>
    <Action ID="PlaceAt" pose_key="place_pose" yaw_deg="0" press_force="10" timeout_ms="1500"/>
    <Action ID="OpenGripper" width="0.09" timeout_ms="800"/>
    <Action ID="Retreat" distance="0.1" timeout_ms="1200"/>
    <Action ID="Wait" timeout_ms="500"/>
  </Sequence>
</BehaviorTree>
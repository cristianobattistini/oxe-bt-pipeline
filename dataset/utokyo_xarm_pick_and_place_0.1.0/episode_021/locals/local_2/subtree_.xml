<BehaviorTree ID="MainTree">
    <Sequence>
        <RetryUntilSuccessful num_attempts="3">
            <DetectObject target="red_plate" timeout_ms="1200"/>
        </RetryUntilSuccessful>
        <MoveAbove target="red_plate" offset_z="0.06" timeout_ms="1200"/>
        <LowerUntilContact speed="slow" max_depth="0.04" force_threshold="5.0" timeout_ms="1200"/>
        <ContactDetected force_threshold="5.0"/>
        <PlaceAt pose_key="red_plate_center" yaw_deg="0" press_force="10" timeout_ms="1200"/>
        <OpenGripper width="0.09" timeout_ms="800"/>
        <Retreat distance="0.1" timeout_ms="800"/>
    </Sequence>
</BehaviorTree>
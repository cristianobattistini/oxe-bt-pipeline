<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="white_plate" timeout_ms="1200"/>
    <MoveTo target="white_plate" timeout_ms="1200"/>
    <ComputeGraspPose target="white_plate" strategy="top" result_key="white_plate_grasp"/>
    <RetryUntilSuccessful num_attempts="2">
      <Pick grasp_key="white_plate_grasp" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <IsGraspStable/>
  </Sequence>
</BehaviorTree>
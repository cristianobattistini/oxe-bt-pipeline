<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="red_plate"/>
      <DetectObject target="red_plate" timeout_ms="800"/>
    </Fallback>
    <MoveAbove target="red_plate" offset_z="0.05" timeout_ms="1200"/>
    <LowerUntilContact speed="normal" max_depth="0.05" force_threshold="10" timeout_ms="1200"/>
    <ContactDetected force_threshold="10"/>
    <OpenGripper width="0.08" timeout_ms="800"/>
    <Retreat distance="0.1" timeout_ms="1200"/>
  </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
    <Sequence>
        <Fallback>
            <IsObjectVisible target="white_plate"/>
            <DetectObject target="white_plate" timeout_ms="800"/>
        </Fallback>
        <OpenGripper width="0.09" timeout_ms="800"/>
        <ComputeGraspPose target="white_plate" strategy="top" result_key="white_grasp"/>
        <ApproachAndAlign target="white_plate" tolerance="0.005" timeout_ms="1200"/>
        <RetryUntilSuccessful num_attempts="2">
            <Pick grasp_key="white_grasp" timeout_ms="1200"/>
        </RetryUntilSuccessful>
        <IsGraspStable/>
    </Sequence>
</BehaviorTree>
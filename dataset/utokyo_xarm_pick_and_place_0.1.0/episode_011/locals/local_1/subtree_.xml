<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="white_plate" timeout_ms="800"/>
    <MoveDelta axis="z" dist="0.05" timeout_ms="1200"/>
    <ApproachAndAlign target="white_plate" tolerance="0.005" timeout_ms="1200"/>
    <ComputeGraspPose target="white_plate" strategy="pinch" result_key="white_plate_grasp"/>
    <RetryUntilSuccessful num_attempts="2">
      <Pick grasp_key="white_plate_grasp" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <ObjectInGripper target="white_plate"/>
  </Sequence>
</BehaviorTree>
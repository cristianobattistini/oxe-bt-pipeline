<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="white_plate" timeout_ms="1200"/>
    <Fallback>
      <PoseAvailable key="grasp_white_plate"/>
      <ComputeGraspPose target="white_plate" strategy="suction" result_key="grasp_white_plate"/>
    </Fallback>
    <MoveAbove target="white_plate" offset_z="0.05" timeout_ms="1200"/>
    <ApproachAndAlign target="white_plate" tolerance="0.01" timeout_ms="1200"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="1200"/>
    <RetryUntilSuccessful num_attempts="3">
      <Pick grasp_key="grasp_white_plate" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <IsGraspStable/>
  </Sequence>
</BehaviorTree>

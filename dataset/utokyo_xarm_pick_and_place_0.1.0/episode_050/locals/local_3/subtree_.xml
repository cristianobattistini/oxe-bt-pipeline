<BehaviorTree ID="MainTree">
    <Sequence>
        <RetryUntilSuccessful num_attempts="3">
            <DetectObject target="white_plate" timeout_ms="1200"/>
        </RetryUntilSuccessful>
        <IsObjectVisible target="white_plate"/>
        <MoveAbove target="white_plate" offset_z="0.04" timeout_ms="1200"/>
        <ComputeGraspPose target="white_plate" strategy="top" result_key="grasp_white_plate"/>
        <ApproachAndAlign target="white_plate" tolerance="0.005" timeout_ms="1200"/>
    </Sequence>
</BehaviorTree>
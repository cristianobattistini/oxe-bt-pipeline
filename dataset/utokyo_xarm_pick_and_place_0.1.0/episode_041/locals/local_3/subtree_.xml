<BehaviorTree ID="MainTree">
    <Sequence>
        <Action ID="DetectObject" target="white_plate" timeout_ms="1200"/>
        <Condition ID="IsObjectVisible" target="white_plate"/>
        <Action ID="ApproachAndAlign" target="white_plate" tolerance="0.005" timeout_ms="1200"/>
        <Action ID="Wait" timeout_ms="400"/>
        <Condition ID="PoseAvailable" key="white_plate_grasp"/>
        <Condition ID="IsAt" target="white_plate"/>
    </Sequence>
</BehaviorTree>
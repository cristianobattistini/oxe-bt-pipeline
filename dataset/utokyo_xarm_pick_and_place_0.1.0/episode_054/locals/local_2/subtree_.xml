<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="2">
      <LowerUntilContact speed="slow" max_depth="0.03" force_threshold="10" timeout_ms="800"/>
    </RetryUntilSuccessful>
    <ContactDetected force_threshold="10"/>
    <PlaceAt pose_key="place_pose" yaw_deg="0" press_force="10" timeout_ms="1200"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
    <Sequence>
        <RetryUntilSuccessful num_attempts="3">
            <Action ID="DetectObject" target="white_plate" timeout_ms="800"/>
        </RetryUntilSuccessful>
        <Action ID="MoveTo" target="white_plate" timeout_ms="1200"/>
        <Action ID="SetTCPYaw" yaw_deg="90"/>
        <Action ID="ApproachAndAlign" target="white_plate" tolerance="0.005" timeout_ms="1500"/>
        <Action ID="ComputeGraspPose" target="white_plate" strategy="pinch" result_key="white_plate_grasp"/>
        <Action ID="Pick" grasp_key="white_plate_grasp" timeout_ms="1200"/>
        <Condition ID="ObjectInGripper" target="white_plate"/>
    </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="red_plate" timeout_ms="1200"/>
    <MoveAbove target="red_plate" offset_z="0.05" timeout_ms="1200"/>
    <RetryUntilSuccessful num_attempts="3">
      <Sequence>
        <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10.0" timeout_ms="1500"/>
        <ContactDetected force_threshold="10.0"/>
      </Sequence>
    </RetryUntilSuccessful>
    <OpenGripper width="0.09" timeout_ms="400"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>
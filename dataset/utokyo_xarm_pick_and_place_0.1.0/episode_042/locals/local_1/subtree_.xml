<BehaviorTree ID="MainTree">
  <Sequence>
    <IsObjectVisible target="white_plate"/>
    <ComputeGraspPose target="white_plate" strategy="top" result_key="grasp_white"/>
    <ApproachAndAlign target="white_plate" tolerance="0.01" timeout_ms="1200"/>
    <RetryUntilSuccessful num_attempts="2">
      <Pick grasp_key="grasp_white" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <ObjectInGripper target="white_plate"/>
  </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
  <Sequence>
    <IsGraspStable/>
    <MoveDelta axis="z" dist="0.1" timeout_ms="800"/>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="red_plate" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <MoveAbove target="red_plate" offset_z="0.08" timeout_ms="1200"/>
    <ApproachAndAlign target="red_plate" tolerance="0.005" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>
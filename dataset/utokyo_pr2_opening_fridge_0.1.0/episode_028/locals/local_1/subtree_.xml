<BehaviorTree ID="MainTree">
  <Sequence>
    <IsObjectVisible target="fridge_handle"/>
    <Timeout timeout_ms="1500">
      <ApproachAndAlign target="fridge_handle" tolerance="0.01" timeout_ms="1500"/>
    </Timeout>
    <SetTCPYaw yaw_deg="90"/>
    <CloseGripper force="20" timeout_ms="800"/>
    <IsGraspStable/>
    <ObjectInGripper target="fridge_handle"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <ContainerOpen target="fridge_door"/>
      <RetryUntilSuccessful num_attempts="3">
        <Sequence>
          <OpenContainer target="fridge_door" container_type="hinged_door" timeout_ms="1500"/>
          <Wait timeout_ms="400"/>
          <ContainerOpen target="fridge_door"/>
        </Sequence>
      </RetryUntilSuccessful>
    </Fallback>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="2">
      <Sequence>
        <Fallback>
          <DetectObject target="grape" timeout_ms="1200"/>
          <ScanForTarget target="grape" pattern="spiral" max_attempts="3" timeout_ms="1500"/>
        </Fallback>
        <MoveAbove target="grape" offset_z="0.02" timeout_ms="1200"/>
        <Timeout timeout_ms="1500">
          <ApproachAndAlign target="grape" tolerance="0.0" timeout_ms="1500"/>
        </Timeout>
      </Sequence>
    </RetryUntilSuccessful>
    <IsAt target="grape"/>
  </Sequence>
</BehaviorTree>
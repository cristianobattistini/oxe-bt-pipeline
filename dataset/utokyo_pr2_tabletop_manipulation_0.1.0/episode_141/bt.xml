<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="grape" timeout_ms="1200"/>
    <IsObjectVisible target="grape"/>
    <OpenGripper width="0.08" timeout_ms="800"/>
    <MoveAbove target="grape" offset_z="0.05" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <ComputeGraspPose target="grape" strategy="pinch" result_key="grape_grasp"/>
    <ApproachAndAlign target="grape" tolerance="0.005" timeout_ms="1200"/>
    <Pick grasp_key="grape_grasp" timeout_ms="1200"/>
    <IsGraspStable/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <MoveAbove target="plate" offset_z="0.06" timeout_ms="1500"/>
    <LowerUntilContact speed="slow" max_depth="0.03" force_threshold="2.0" timeout_ms="1200"/>
    <OpenGripper width="0.06" timeout_ms="800"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="plate"/>
      <DetectObject target="plate" timeout_ms="1200"/>
    </Fallback>
    <PoseAvailable key="plate_center"/>
    <MoveAbove target="plate" offset_z="0.05" timeout_ms="1200"/>
    <ApproachAndAlign target="plate" tolerance="0.01" timeout_ms="1200"/>
    <PlaceAt pose_key="plate_center" yaw_deg="0" press_force="10" timeout_ms="1200"/>
    <IsAt target="plate"/>
    <AtOrientation yaw_deg="0"/>
  </Sequence>
</BehaviorTree>

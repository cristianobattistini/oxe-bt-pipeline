<BehaviorTree ID="MainTree">
  <Sequence>
    <Action ID="DetectObject" target="plate" timeout_ms="1200"/>
    <Action ID="MoveAbove" target="plate" offset_z="0.05" timeout_ms="1200"/>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.05" force_threshold="10.0" timeout_ms="1500"/>
    <Condition ID="ContactDetected" force_threshold="10.0"/>
  </Sequence>
</BehaviorTree>
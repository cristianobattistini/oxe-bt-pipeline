<BehaviorTree ID="MainTree">
  <Sequence>
    <IsObjectVisible target="plate"/>
    <Fallback>
      <IsAt target="plate"/>
      <MoveAbove target="plate" offset_z="0.06" timeout_ms="1200"/>
    </Fallback>
    <LowerUntilContact speed="slow" max_depth="0.03" force_threshold="10" timeout_ms="1200"/>
    <ContactDetected force_threshold="10"/>
  </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="bread" timeout_ms="1200"/>
    <DetectObject target="plate" timeout_ms="1200"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <MoveAbove target="bread" offset_z="0.05" timeout_ms="1500"/>
    <ComputeGraspPose target="bread" strategy="pinch" result_key="bread_grasp"/>
    <PoseAvailable key="bread_grasp"/>
    <ApproachAndAlign target="bread" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="800"/>
    <CloseGripper force="20" timeout_ms="1200"/>
    <IsGraspStable/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <MoveAbove target="plate" offset_z="0.05" timeout_ms="1500"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="800"/>
    <OpenGripper width="0.09" timeout_ms="800"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

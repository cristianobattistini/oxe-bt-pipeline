<BehaviorTree ID="MainTree">
  <Sequence>
    <OpenGripper width="0.06" timeout_ms="800"/>
    <Fallback>
      <IsObjectVisible target="grape"/>
      <RetryUntilSuccessful num_attempts="3">
        <DetectObject target="grape" timeout_ms="800"/>
      </RetryUntilSuccessful>
    </Fallback>
    <MoveAbove target="grape" offset_z="0.05" timeout_ms="1200"/>
    <ComputeGraspPose target="grape" strategy="pinch" result_key="grasp_grape"/>
    <PoseAvailable key="grasp_grape"/>
    <ApproachAndAlign target="grasp_grape" tolerance="0.01" timeout_ms="1500"/>
    <LowerUntilContact speed="slow" max_depth="0.02" force_threshold="10.0" timeout_ms="1200"/>
    <CloseGripper force="10" timeout_ms="800"/>
    <MoveDelta axis="z" dist="0.05" timeout_ms="800"/>
    <ObjectInGripper target="grape"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

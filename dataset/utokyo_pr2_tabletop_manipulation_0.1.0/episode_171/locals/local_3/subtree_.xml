<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="bread"/>
      <DetectObject target="bread" timeout_ms="1200"/>
    </Fallback>
    <MoveAbove target="bread" offset_z="0.03" timeout_ms="1500"/>
    <RetryUntilSuccessful num_attempts="2">
      <ApproachAndAlign target="bread" tolerance="0.01" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <Wait timeout_ms="400"/>
    <IsAt target="bread"/>
  </Sequence>
</BehaviorTree>
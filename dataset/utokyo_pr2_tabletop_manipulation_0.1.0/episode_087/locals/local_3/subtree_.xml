<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <Condition ID="IsObjectVisible" target="grape"/>
      <RetryUntilSuccessful num_attempts="3">
        <ScanForTarget target="grape" pattern="raster" max_attempts="3" timeout_ms="800"/>
      </RetryUntilSuccessful>
    </Fallback>
    <DetectObject target="grape" timeout_ms="800"/>
    <MoveAbove target="grape" offset_z="0.02" timeout_ms="1200"/>
    <Condition ID="IsAt" target="grape"/>
  </Sequence>
</BehaviorTree>
<BehaviorTree ID="MainTree">
  <Sequence>
    <Condition ID="ObjectInGripper" target="grape_cluster"/>
    <Action ID="ApproachAndAlign" target="plate_center" tolerance="0.005" timeout_ms="1200"/>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="1200"/>
    <Condition ID="ContactDetected" force_threshold="10"/>
  </Sequence>
</BehaviorTree>
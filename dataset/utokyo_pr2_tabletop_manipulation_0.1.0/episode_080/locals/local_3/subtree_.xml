<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="grape"/>
      <ScanForTarget target="grape" pattern="raster" max_attempts="3" timeout_ms="1500"/>
    </Fallback>
    <DetectObject target="grape" timeout_ms="800"/>
    <RetryUntilSuccessful num_attempts="2">
      <ApproachAndAlign target="grape" tolerance="0.01" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <IsAt target="grape"/>
  </Sequence>
</BehaviorTree>
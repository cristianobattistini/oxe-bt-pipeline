<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="plate"/>
      <RetryUntilSuccessful num_attempts="3">
        <DetectObject target="plate" timeout_ms="1200"/>
      </RetryUntilSuccessful>
    </Fallback>
    <ApproachAndAlign target="plate" tolerance="0.005" timeout_ms="1200"/>
    <MoveAbove target="plate" offset_z="0.03" timeout_ms="1200"/>
    <IsAt target="plate"/>
  </Sequence>
</BehaviorTree>
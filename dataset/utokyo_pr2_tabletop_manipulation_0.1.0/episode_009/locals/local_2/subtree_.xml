<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="plate"/>
      <RetryUntilSuccessful num_attempts="3">
        <DetectObject target="plate" timeout_ms="800"/>
      </RetryUntilSuccessful>
    </Fallback>
    <MoveAbove target="plate" offset_z="0.10" timeout_ms="1500"/>
    <IsAt target="plate"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Action ID="DetectObject" target="t_block" timeout_ms="800"/>
    <Action ID="DetectObject" target="target_goal" timeout_ms="800"/>
    <Action ID="ApproachAndAlign" target="t_block" tolerance="0.005" timeout_ms="1200"/>
    <Action ID="SetTCPYaw" yaw_deg="180"/>
    <Action ID="Push" target="t_block_towards_goal" distance="0.1" direction_deg="180" timeout_ms="800"/>
    <Condition ID="IsObjectVisible" target="t_block"/>
  </Sequence>
</BehaviorTree>

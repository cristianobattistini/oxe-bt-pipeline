<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="t_block"/>
      <ScanForTarget target="t_block" pattern="raster" max_attempts="3" timeout_ms="1500"/>
    </Fallback>
    <SetTCPYaw yaw_deg="0"/>
    <ApproachAndAlign target="t_block" tolerance="0.0" timeout_ms="1200"/>
    <Push target="t_block" distance="0.1" direction_deg="180" timeout_ms="1200"/>
    <Wait timeout_ms="500"/>
    <IsObjectVisible target="t_block"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="1200"/>
    <DetectObject target="target_zone" timeout_ms="1200"/>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="2000"/>
    <RetryUntilSuccessful num_attempts="3">
      <Push target="t_block" distance="0.2" direction_deg="90" timeout_ms="2000"/>
    </RetryUntilSuccessful>
    <IsAt target="target_zone"/>
    <Retreat distance="0.1" timeout_ms="1200"/>
  </Sequence>
</BehaviorTree>

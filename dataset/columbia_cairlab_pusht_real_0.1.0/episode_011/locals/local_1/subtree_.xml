    <SetTCPYaw yaw_deg="90"/>
    <LowerUntilContact speed="normal" max_depth="0.02" force_threshold="10" timeout_ms="800"/>
    <ContactDetected force_threshold="10"/>
  </Sequence>
</BehaviorTree>
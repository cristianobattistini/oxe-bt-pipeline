<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="1200"/>
    <DetectObject target="target_goal" timeout_ms="1200"/>
    <MoveAbove target="t_block" offset_z="0.02" timeout_ms="1200"/>
    <ApproachAndAlign target="t_block" tolerance="0.005" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <Push target="t_block" distance="0.2" direction_deg="180" timeout_ms="1500"/>
    <ApproachAndAlign target="target_goal" tolerance="0.01" timeout_ms="1500"/>
    <Push target="t_block" distance="0.1" direction_deg="180" timeout_ms="1200"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

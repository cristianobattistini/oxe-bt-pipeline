<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="t_block" timeout_ms="800"/>
    </RetryUntilSuccessful>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="target_marker" timeout_ms="800"/>
    </RetryUntilSuccessful>
    <ApproachAndAlign target="t_block" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="45"/>
    <AtOrientation yaw_deg="45"/>
    <Push target="t_block" distance="0.2" direction_deg="45" timeout_ms="1500"/>
    <IsAt target="target_marker"/>
  </Sequence>
</BehaviorTree>
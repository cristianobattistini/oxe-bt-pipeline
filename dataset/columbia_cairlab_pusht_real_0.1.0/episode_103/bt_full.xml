<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <DetectObject target="t_block" timeout_ms="800"/>
      <RetryUntilSuccessful num_attempts="3">
        <Sequence>
          <ScanForTarget target="t_block" pattern="raster" max_attempts="3" timeout_ms="800"/>
          <DetectObject target="t_block" timeout_ms="800"/>
        </Sequence>
      </RetryUntilSuccessful>
    </Fallback>
    <Fallback>
      <DetectObject target="target_T_slot" timeout_ms="800"/>
      <RetryUntilSuccessful num_attempts="3">
        <Sequence>
          <ScanForTarget target="target_T_slot" pattern="raster" max_attempts="3" timeout_ms="800"/>
          <DetectObject target="target_T_slot" timeout_ms="800"/>
        </Sequence>
      </RetryUntilSuccessful>
    </Fallback>
    <MoveAbove target="t_block" offset_z="0.03" timeout_ms="800"/>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="800"/>
    <SetTCPYaw yaw_deg="90"/>
    <Repeat num_cycles="2">
      <Push target="t_block" distance="0.1" direction_deg="0" timeout_ms="800"/>
    </Repeat>
    <ApproachAndAlign target="target_T_slot" tolerance="0.02" timeout_ms="800"/>
    <Push target="t_block" distance="0.05" direction_deg="45" timeout_ms="800"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

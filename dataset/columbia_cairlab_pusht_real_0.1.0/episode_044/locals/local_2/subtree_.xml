<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="target_zone" timeout_ms="1200"/>
    <Fallback>
      <IsAt target="target_zone"/>
      <Sequence>
        <ApproachAndAlign target="target_zone" tolerance="0.01" timeout_ms="1200"/>
        <Push target="t_block" distance="0.05" direction_deg="225" timeout_ms="800"/>
        <IsAt target="target_zone"/>
      </Sequence>
    </Fallback>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="800"/>
    <DetectObject target="target_T_outline" timeout_ms="800"/>
    <MoveAbove target="t_block" offset_z="0.03" timeout_ms="1200"/>
    <ApproachAndAlign target="t_block" tolerance="0.005" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <Push target="t_block" distance="0.1" direction_deg="180" timeout_ms="1200"/>
    <Push target="t_block" distance="0.05" direction_deg="270" timeout_ms="1200"/>
    <ApproachAndAlign target="target_T_outline" tolerance="0.005" timeout_ms="1500"/>
    <IsAt target="target_T_outline"/>
    <Wait timeout_ms="400"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="t_block"/>
      <ScanForTarget target="t_block" pattern="raster" max_attempts="3" timeout_ms="1500"/>
    </Fallback>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <Push target="t_block" distance="0.2" direction_deg="90" timeout_ms="1500"/>
    <DetectObject target="t_block" timeout_ms="500"/>
  </Sequence>
</BehaviorTree>
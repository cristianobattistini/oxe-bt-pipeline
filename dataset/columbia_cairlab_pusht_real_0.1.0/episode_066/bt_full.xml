<BehaviorTree ID="MainTree">
  <Sequence>
    <SetBlackboard key="control_rate_hz" value="10"/>
    <DetectObject target="t_block" timeout_ms="800"/>
    <DetectObject target="goal_T" timeout_ms="800"/>
    <MoveAbove target="t_block" offset_z="0.02" timeout_ms="800"/>
    <ApproachAndAlign target="t_block" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <Push target="t_block" distance="0.2" direction_deg="90" timeout_ms="1500"/>
    <Push target="t_block" distance="0.1" direction_deg="45" timeout_ms="1200"/>
    <ApproachAndAlign target="goal_T" tolerance="0.01" timeout_ms="1200"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

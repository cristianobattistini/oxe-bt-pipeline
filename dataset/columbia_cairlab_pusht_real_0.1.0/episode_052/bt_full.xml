<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block_gray" timeout_ms="1200"/>
    <DetectObject target="target_green" timeout_ms="1200"/>
    <MoveAbove target="t_block_gray" offset_z="0.02" timeout_ms="800"/>
    <ApproachAndAlign target="t_block_gray" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <LowerUntilContact speed="slow" max_depth="0.01" force_threshold="10.0" timeout_ms="800"/>
    <Push target="t_block_gray" distance="0.1" direction_deg="225" timeout_ms="1200"/>
    <Push target="t_block_gray" distance="0.05" direction_deg="180" timeout_ms="1200"/>
    <ApproachAndAlign target="target_green" tolerance="0.01" timeout_ms="1200"/>
    <IsAt target="target_green"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

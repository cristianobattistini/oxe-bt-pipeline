<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="800"/>
    <DetectObject target="target_zone" timeout_ms="800"/>
    <MoveAbove target="t_block" offset_z="0.02" timeout_ms="1200"/>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="1200"/>
    <LowerUntilContact speed="slow" max_depth="0.01" force_threshold="10" timeout_ms="800"/>
    <SetTCPYaw yaw_deg="45"/>
    <Push target="t_block" distance="0.1" direction_deg="45" timeout_ms="1500"/>
    <Push target="t_block" distance="0.1" direction_deg="90" timeout_ms="1500"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="800"/>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="45"/>
    <AtOrientation yaw_deg="45"/>
    <Push target="t_block" distance="0.1" direction_deg="45" timeout_ms="1500"/>
    <IsObjectVisible target="t_block"/>
  </Sequence>
</BehaviorTree>

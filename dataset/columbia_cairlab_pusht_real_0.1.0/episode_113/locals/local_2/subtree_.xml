<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="block_T"/>
      <DetectObject target="block_T" timeout_ms="800"/>
    </Fallback>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="block_T" tolerance="0.005" timeout_ms="1200"/>
    <Push target="block_T" distance="0.05" direction_deg="0" timeout_ms="1200"/>
    <DetectObject target="block_T" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="block_T" timeout_ms="800"/>
    <DetectObject target="goal_T" timeout_ms="800"/>
    <MoveAbove target="block_T" offset_z="0.02" timeout_ms="800"/>
    <ApproachAndAlign target="block_T" tolerance="0.005" timeout_ms="800"/>
    <LowerUntilContact speed="slow" max_depth="0.02" force_threshold="10" timeout_ms="800"/>
    <SetTCPYaw yaw_deg="180"/>
    <Push target="block_T" distance="0.2" direction_deg="180" timeout_ms="1500"/>
    <ApproachAndAlign target="goal_T" tolerance="0.02" timeout_ms="1200"/>
    <IsAt target="goal_T"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible ID="IsObjectVisible" target="t_block"/>
      <DetectObject ID="DetectObject" target="t_block" timeout_ms="800"/>
    </Fallback>
    <ApproachAndAlign ID="ApproachAndAlign" target="t_block" tolerance="0.01" timeout_ms="1200"/>
    <Push ID="Push" target="t_block" distance="0.2" direction_deg="90" timeout_ms="1200"/>
    <IsAt ID="IsAt" target="t_target"/>
  </Sequence>
</BehaviorTree>

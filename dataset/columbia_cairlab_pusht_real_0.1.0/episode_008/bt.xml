<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="800"/>
    <DetectObject target="goal_marker" timeout_ms="800"/>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <Push target="t_block" distance="0.1" direction_deg="90" timeout_ms="1500"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

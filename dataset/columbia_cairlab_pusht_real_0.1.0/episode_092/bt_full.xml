<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="block_T" timeout_ms="800"/>
    <DetectObject target="target_T" timeout_ms="800"/>
    <MoveAbove target="block_T" offset_z="0.03" timeout_ms="800"/>
    <ApproachAndAlign target="block_T" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="0"/>
    <RetryUntilSuccessful num_attempts="3">
      <Push target="block_T" distance="0.1" direction_deg="0" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <ApproachAndAlign target="target_T" tolerance="0.01" timeout_ms="1200"/>
    <Retreat distance="0.1" timeout_ms="500"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

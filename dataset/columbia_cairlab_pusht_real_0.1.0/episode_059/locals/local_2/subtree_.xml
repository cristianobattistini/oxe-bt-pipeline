<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="800"/>
    <DetectObject target="goal_target" timeout_ms="800"/>
    <ApproachAndAlign target="t_block" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="180"/>
    <Push target="t_block" distance="0.2" direction_deg="180" timeout_ms="1500"/>
    <ApproachAndAlign target="goal_target" tolerance="0.01" timeout_ms="1200"/>
    <Wait timeout_ms="400"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

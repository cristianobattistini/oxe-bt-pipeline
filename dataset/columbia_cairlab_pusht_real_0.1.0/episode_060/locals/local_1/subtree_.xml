<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="t_block" timeout_ms="800"/>
    <MoveAbove target="t_block" offset_z="0.03" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="t_block" tolerance="0.005" timeout_ms="1200"/>
    <LowerUntilContact speed="slow" max_depth="0.01" force_threshold="10" timeout_ms="800"/>
    <ContactDetected force_threshold="10"/>
  </Sequence>
</BehaviorTree>

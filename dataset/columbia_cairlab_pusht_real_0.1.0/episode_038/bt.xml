<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="t_block" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="target_zone" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="t_block" tolerance="0.01" timeout_ms="1200"/>
    <Timeout timeout_ms="1500">
      <Push target="t_block" distance="0.1" direction_deg="90" timeout_ms="1200"/>
    </Timeout>
    <Timeout timeout_ms="1500">
      <Push target="t_block" distance="0.1" direction_deg="90" timeout_ms="1200"/>
    </Timeout>
    <IsAt target="target_zone"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

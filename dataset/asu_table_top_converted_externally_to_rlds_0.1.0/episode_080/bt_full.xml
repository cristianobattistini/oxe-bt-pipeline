<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="brown_object" timeout_ms="2000"/>
    <OpenGripper width="0.08" timeout_ms="800"/>
    <MoveAbove target="brown_object" offset_z="0.08" timeout_ms="1500"/>
    <ApproachAndAlign target="brown_object" tolerance="0.01" timeout_ms="2000"/>
    <ComputeGraspPose target="brown_object" strategy="top" result_key="grasp_pose"/>
    <GraspAtPose pose_key="grasp_pose" strategy="top" timeout_ms="1500"/>
    <CloseGripper force="20" timeout_ms="1000"/>
    <ObjectInGripper target="brown_object"/>
    <RaiseHeld target="brown_object" distance="0.10" timeout_ms="1000"/>
    <HoldObject target="brown_object" duration_ms="2000"/>
  </Sequence>
</BehaviorTree>

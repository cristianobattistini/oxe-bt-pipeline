<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="maroon_square" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <OpenGripper width="0.06" timeout_ms="1000"/>
    <MoveAbove target="maroon_square" offset_z="0.05" timeout_ms="1500"/>
    <ApproachAndAlign target="maroon_square" tolerance="0.005" timeout_ms="2000"/>
    <Pick grasp_key="std_top" timeout_ms="2000"/>
    <IsGraspStable/>
    <RaiseHeld target="maroon_square" distance="0.10" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>

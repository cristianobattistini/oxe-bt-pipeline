<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="clear_spot_table" timeout_ms="1500"/>
    <MoveAbove target="clear_spot_table" offset_z="0.10" timeout_ms="2000"/>
    <ObjectInGripper target="pepsi_bottle"/>
    <LowerUntilContact speed="slow" max_depth="0.10" force_threshold="5" timeout_ms="2000"/>
    <OpenGripper width="0.06" timeout_ms="400"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
    <Wait timeout_ms="500"/>
    <SetBlackboard key="last_action" value="put_down_pepsi_bottle"/>
  </Sequence>
</BehaviorTree>

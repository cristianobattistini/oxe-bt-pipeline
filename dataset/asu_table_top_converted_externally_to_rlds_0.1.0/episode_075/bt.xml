<BehaviorTree ID="MainTree">
  <Sequence>
    <OpenGripper width="0.06" timeout_ms="500"/>
    <DetectObject target="maroon_square" timeout_ms="1500"/>
    <ComputeGraspPose target="maroon_square" strategy="top" result_key="goal_pose"/>
    <GraspAtPose pose_key="goal_pose" strategy="top" timeout_ms="2000"/>
    <ObjectInGripper target="maroon_square"/>
    <RaiseHeld target="maroon_square" distance="0.10" timeout_ms="1000"/>
    <PlaceOnSurface target="maroon_square" area_id="table" yaw_deg="0" press_force="5" timeout_ms="1500"/>
    <OpenGripper width="0.06" timeout_ms="500"/>
    <Retreat distance="0.10" timeout_ms="800"/>
    <Wait timeout_ms="500"/>
  </Sequence>
</BehaviorTree>

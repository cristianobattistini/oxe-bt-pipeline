<BehaviorTree ID="MainTree">
  <Sequence>
    <Condition ID="ObjectInGripper" target="white_carton"/>
    <Action ID="ApproachAndAlign" target="table" tolerance="0.01" timeout_ms="2000"/>
    <Action ID="MoveAbove" target="table" offset_z="0.10" timeout_ms="1500"/>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.05" force_threshold="5" timeout_ms="1500"/>
    <Condition ID="ContactDetected" force_threshold="5"/>
  </Sequence>
</BehaviorTree>
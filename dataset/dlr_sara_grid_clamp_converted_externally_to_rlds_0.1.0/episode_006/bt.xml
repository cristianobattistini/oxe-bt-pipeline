<BehaviorTree ID="MainTree">
  <Sequence>
    <ObjectInGripper target="grid_clamp"/>
    <MoveAbove target="place_on_table" offset_z="0.05" timeout_ms="2000"/>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="place_on_table" tolerance="0.005" timeout_ms="2000"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="5" timeout_ms="2000"/>
    <PlaceAtTol pose_key="place_on_table" yaw_deg="90" press_force="10" tolerance="0.005" timeout_ms="2000"/>
    <OpenGripper width="0.04" timeout_ms="800"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <ObjectInGripper target="grid_clamp"/>
    <DetectObject target="place_slot" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <MoveAbove target="place_slot" offset_z="0.05" timeout_ms="2000"/>
    <ApproachAndAlign target="place_slot" tolerance="0.01" timeout_ms="1500"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="5" timeout_ms="1500"/>
    <OpenGripper width="0.06" timeout_ms="800"/>
    <Wait timeout_ms="500"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

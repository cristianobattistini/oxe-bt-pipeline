<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="grid_clamp" timeout_ms="1500"/>
    <MoveAbove target="grid_clamp" offset_z="0.10" timeout_ms="1500"/>
    <ComputeGraspPose target="grid_clamp" strategy="top" result_key="goal_pose"/>
    <GraspAtPose pose_key="goal_pose" strategy="top" timeout_ms="2000"/>
    <RaiseHeld target="grid_clamp" distance="0.10" timeout_ms="1500"/>
    <PlaceOnSurface target="grid_clamp" area_id="grid_fixture" yaw_deg="0" press_force="10" timeout_ms="2000"/>
    <OpenGripper width="0.06" timeout_ms="800"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="grid_clamp" timeout_ms="1500"/>
    <ComputeGraspPose target="grid_clamp" strategy="top" result_key="goal_pose"/>
    <ApproachAndAlign target="grid_clamp" tolerance="0.01" timeout_ms="2000"/>
    <CloseGripper force="20" timeout_ms="1000"/>
    <RaiseHeld target="grid_clamp" distance="0.10" timeout_ms="1500"/>
    <MoveAbove target="grid_clamp" offset_z="0.10" timeout_ms="1500"/>
    <PlaceOnSurface target="grid_clamp" area_id="grid" yaw_deg="0" press_force="10" timeout_ms="2000"/>
    <OpenGripper width="0.08" timeout_ms="800"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

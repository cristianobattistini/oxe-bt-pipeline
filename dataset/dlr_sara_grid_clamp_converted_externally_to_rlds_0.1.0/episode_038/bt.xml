<BehaviorTree ID="MainTree">
  <Sequence>
    <ObjectInGripper target="grid_clamp"/>
    <SetTCPYaw yaw_deg="0"/>
    <MoveAbove target="grid_fixture" offset_z="0.05" timeout_ms="2000"/>
    <ApproachAndAlign target="grid_fixture" tolerance="0.01" timeout_ms="2000"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="2000"/>
    <PlaceAtTol pose_key="goal_pose" yaw_deg="0" press_force="10" tolerance="0.005" timeout_ms="1500"/>
    <OpenGripper width="0.04" timeout_ms="800"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

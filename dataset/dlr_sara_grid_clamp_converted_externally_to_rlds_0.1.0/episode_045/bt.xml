<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="grid_clamp_goal" timeout_ms="2000"/>
    <MoveAbove target="grid_clamp_goal" offset_z="0.05" timeout_ms="2000"/>
    <ApproachAndAlign target="grid_clamp_goal" tolerance="0.005" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="5" timeout_ms="2000"/>
    <OpenGripper width="0.04" timeout_ms="500"/>
    <Wait timeout_ms="500"/>
    <Retreat distance="0.10" timeout_ms="1000"/>
  </Sequence>
</BehaviorTree>

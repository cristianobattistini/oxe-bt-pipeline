<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="grid_clamp" timeout_ms="1500"/>
    <MoveAbove target="grid_clamp" offset_z="0.05" timeout_ms="2000"/>
    <ApproachAndAlign target="grid_clamp" tolerance="0.005" timeout_ms="1500"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="2000"/>
    <PlaceDown target="grid_clamp" timeout_ms="1500"/>
    <OpenGripper width="0.08" timeout_ms="1000"/>
    <Retreat distance="0.10" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>

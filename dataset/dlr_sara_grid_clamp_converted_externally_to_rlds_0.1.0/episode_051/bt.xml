<BehaviorTree ID="MainTree">
  <Sequence>
    <IsObjectVisible target="grid_clamp_site"/>
    <ObjectInGripper target="grid_clamp"/>
    <MoveAbove target="grid_clamp_site" offset_z="0.05" timeout_ms="1500"/>
    <ApproachAndAlign target="grid_clamp_site" tolerance="0.01" timeout_ms="2000"/>
    <SetTCPYaw yaw_deg="90"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="2000"/>
    <OpenGripper width="0.08" timeout_ms="800"/>
    <Wait timeout_ms="500"/>
    <MoveDelta axis="z" dist="0.10" timeout_ms="1500"/>
    <Retreat distance="0.10" timeout_ms="1500"/>
  </Sequence>
</BehaviorTree>

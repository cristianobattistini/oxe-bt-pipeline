<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <Condition ID="IsObjectVisible" target="dishwasher_handle"/>
      <Action ID="DetectObject" target="dishwasher_handle" timeout_ms="1200"/>
    </Fallback>
    <Action ID="MoveTo" target="dishwasher_front" timeout_ms="1200"/>
    <Action ID="ApproachAndAlign" target="dishwasher_handle" tolerance="0.01" timeout_ms="1200"/>
    <Action ID="CloseGripper" force="20" timeout_ms="800"/>
    <Condition ID="IsGraspStable"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="lid" timeout_ms="1500"/>
    <MoveAbove target="lid" offset_z="0.05" timeout_ms="1200"/>
    <OpenGripper width="0.09" timeout_ms="800"/>
    <ComputeGraspPose target="lid" strategy="top" result_key="lid_grasp"/>
    <ApproachAndAlign target="lid" tolerance="0.01" timeout_ms="1200"/>
    <LowerUntilContact speed="slow" max_depth="0.03" force_threshold="10" timeout_ms="1200"/>
    <CloseGripper force="20" timeout_ms="1200"/>
    <IsGraspStable/>
    <MoveDelta axis="z" dist="0.1" timeout_ms="800"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="500"/>
  </Sequence>
</BehaviorTree>

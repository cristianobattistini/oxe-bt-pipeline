<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="dishwasher_handle" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <ApproachAndAlign target="dishwasher_handle" tolerance="0.01" timeout_ms="1500"/>
    <ComputeGraspPose target="dishwasher_handle" strategy="pull" result_key="grasp_dishwasher_handle"/>
    <PoseAvailable key="grasp_dishwasher_handle"/>
  </Sequence>
</BehaviorTree>

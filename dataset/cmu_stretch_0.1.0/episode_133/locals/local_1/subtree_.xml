<BehaviorTree ID="MainTree">
  <Sequence>
    <PoseAvailable key="lid_grasp"/>
    <AtOrientation yaw_deg="90"/>
    <ApproachAndAlign target="green_bin_lid" tolerance="0.01" timeout_ms="1500"/>
    <RetryUntilSuccessful num_attempts="3">
      <Pick grasp_key="lid_grasp" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <IsGraspStable/>
    <ObjectInGripper target="green_bin_lid"/>
  </Sequence>
</BehaviorTree>

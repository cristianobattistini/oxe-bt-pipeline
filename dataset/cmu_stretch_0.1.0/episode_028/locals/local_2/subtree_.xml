<BehaviorTree ID="MainTree">
  <Sequence>
    <Condition ID="IsObjectVisible" target="drawer_handle"/>
    <Action ID="ApproachAndAlign" target="drawer_handle" tolerance="0.01" timeout_ms="1500"/>
    <Action ID="SetTCPYaw" yaw_deg="0"/>
    <Condition ID="AtOrientation" yaw_deg="0"/>
    <Action ID="CloseGripper" force="30" timeout_ms="1200"/>
    <Condition ID="ObjectInGripper" target="drawer_handle"/>
  </Sequence>
</BehaviorTree>

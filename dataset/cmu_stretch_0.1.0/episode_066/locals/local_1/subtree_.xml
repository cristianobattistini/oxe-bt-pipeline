<BehaviorTree ID="MainTree">
  <Sequence>
    <IsObjectVisible target="dishwasher_handle"/>
    <PoseAvailable key="grasp_handle"/>
    <ApproachAndAlign target="dishwasher_handle" tolerance="0.01" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <RetryUntilSuccessful num_attempts="3">
      <Pick grasp_key="grasp_handle" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <ObjectInGripper target="dishwasher_handle"/>
    <IsGraspStable/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <ScanForTarget target="dishwasher_handle" pattern="line" max_attempts="3" timeout_ms="1200"/>
    <DetectObject target="dishwasher_handle" timeout_ms="800"/>
    <MoveTo target="dishwasher_handle_pregrasp" timeout_ms="1500"/>
    <ApproachAndAlign target="dishwasher_handle" tolerance="0.005" timeout_ms="1200"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <SetTCPYaw yaw_deg="90"/>
    <MoveDelta axis="x" dist="0.05" timeout_ms="500"/>
    <CloseGripper force="20" timeout_ms="800"/>
    <RetryUntilSuccessful num_attempts="3">
      <Sequence>
        <OpenContainer target="dishwasher" container_type="hinged_door" timeout_ms="1500"/>
        <ContainerOpen target="dishwasher"/>
      </Sequence>
    </RetryUntilSuccessful>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

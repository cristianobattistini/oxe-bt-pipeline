<BehaviorTree ID="MainTree">
  <Sequence>
    <ScanForTarget target="green_bin" pattern="arc" max_attempts="3" timeout_ms="1500"/>
    <DetectObject target="bin_lid" timeout_ms="1200"/>
    <MoveTo target="green_bin" timeout_ms="1500"/>
    <MoveAbove target="bin_lid" offset_z="0.06" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="bin_lid" tolerance="0.01" timeout_ms="1200"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="10" timeout_ms="1500"/>
    <MoveDelta axis="z" dist="0.1" timeout_ms="800"/>
    <OpenContainer target="green_bin" container_type="bin_lid" timeout_ms="1500"/>
    <ContainerOpen target="green_bin"/>
    <Retreat distance="0.1" timeout_ms="800"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

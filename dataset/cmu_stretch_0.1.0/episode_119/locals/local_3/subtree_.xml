<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="lid" timeout_ms="1500"/>
    <IsObjectVisible target="lid"/>
    <SetTCPYaw yaw_deg="90"/>
    <RetryUntilSuccessful num_attempts="2">
      <ApproachAndAlign target="lid_handle" tolerance="0.005" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <IsAt target="lid_handle"/>
    <AtOrientation yaw_deg="90"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <OpenGripper width="0.09" timeout_ms="800"/>
    <DetectObject target="drawer_handle" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="drawer_handle" tolerance="0.005" timeout_ms="1500"/>
    <CloseGripper force="30" timeout_ms="1200"/>
    <OpenContainer target="drawer" container_type="drawer" timeout_ms="1500"/>
    <Retreat distance="0.1" timeout_ms="1200"/>
    <Wait timeout_ms="400"/>
  </Sequence>
</BehaviorTree>

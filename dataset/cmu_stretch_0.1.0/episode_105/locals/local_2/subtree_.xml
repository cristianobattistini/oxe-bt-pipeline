<BehaviorTree ID="MainTree">
  <Sequence>
    <Condition ID="IsObjectVisible" target="drawer_handle"/>
    <Action ID="ApproachAndAlign" target="drawer_handle" tolerance="0.005" timeout_ms="1200"/>
    <Action ID="MoveDelta" axis="x" dist="0.1" timeout_ms="800"/>
    <Condition ID="ContainerOpen" target="drawer"/>
  </Sequence>
</BehaviorTree>

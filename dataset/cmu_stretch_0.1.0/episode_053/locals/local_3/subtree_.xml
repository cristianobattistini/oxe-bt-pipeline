<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="lid" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="0"/>
    <RetryUntilSuccessful num_attempts="3">
      <ApproachAndAlign target="lid" tolerance="0.01" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <AtOrientation yaw_deg="0"/>
  </Sequence>
</BehaviorTree>

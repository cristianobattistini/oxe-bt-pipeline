<BehaviorTree ID="MainTree">
  <Sequence>
    <Action ID="DetectObject" target="green_bin" timeout_ms="1500"/>
    <Action ID="OpenGripper" width="0.09" timeout_ms="800"/>
    <Action ID="MoveAbove" target="green_bin_lid_edge" offset_z="0.02" timeout_ms="1500"/>
    <Action ID="ApproachAndAlign" target="green_bin_lid_edge" tolerance="0.01" timeout_ms="1500"/>
    <Action ID="LowerUntilContact" speed="slow" max_depth="0.05" force_threshold="5.0" timeout_ms="1200"/>
    <Action ID="CloseGripper" force="10" timeout_ms="800"/>
    <Action ID="OpenContainer" target="green_bin" container_type="bin_lid" timeout_ms="1500"/>
    <Condition ID="ContainerOpen" target="green_bin"/>
    <Action ID="Retreat" distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

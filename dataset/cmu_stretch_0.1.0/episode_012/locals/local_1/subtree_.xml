<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="door_handle" timeout_ms="1200"/>
    <MoveTo target="door_handle" timeout_ms="1200"/>
    <RetryUntilSuccessful num_attempts="3">
      <ApproachAndAlign target="door_handle" tolerance="0.005" timeout_ms="1500"/>
    </RetryUntilSuccessful>
    <IsAt target="door_handle"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <IsObjectVisible target="drawer_handle"/>
      <RetryUntilSuccessful num_attempts="3">
        <ScanForTarget target="drawer_handle" pattern="raster" max_attempts="3" timeout_ms="1200"/>
      </RetryUntilSuccessful>
    </Fallback>
    <MoveTo target="drawer_handle" timeout_ms="1500"/>
    <SetTCPYaw yaw_deg="90"/>
    <AtOrientation yaw_deg="90"/>
  </Sequence>
</BehaviorTree>

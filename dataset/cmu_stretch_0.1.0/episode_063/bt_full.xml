<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="green_bin" timeout_ms="1200"/>
    <MoveAbove target="green_bin_lid" offset_z="0.08" timeout_ms="1500"/>
    <ApproachAndAlign target="lid_edge" tolerance="0.01" timeout_ms="1500"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <LowerUntilContact speed="slow" max_depth="0.05" force_threshold="5.0" timeout_ms="1200"/>
    <CloseGripper force="20" timeout_ms="800"/>
    <SetTCPYaw yaw_deg="90"/>
    <OpenContainer target="green_bin" container_type="bin_lid" timeout_ms="1500"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <Retreat distance="0.1" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

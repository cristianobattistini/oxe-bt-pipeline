<BehaviorTree ID="MainTree">
  <Sequence>
    <MoveTo target="cabinet_front" timeout_ms="2000"/>
    <RetryUntilSuccessful num_attempts="3">
      <DetectObject target="door_handle" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <IsObjectVisible target="door_handle"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <ComputeGraspPose target="door_handle" strategy="pull" result_key="grasp_handle"/>
    <ApproachAndAlign target="door_handle" tolerance="0.005" timeout_ms="1200"/>
    <SetTCPYaw yaw_deg="90"/>
    <AtOrientation yaw_deg="90"/>
    <CloseGripper force="20" timeout_ms="1200"/>
    <IsGraspStable/>
    <OpenContainer target="cabinet_door" container_type="hinged_door" timeout_ms="1500"/>
    <ContainerOpen target="cabinet_door"/>
    <OpenGripper width="0.09" timeout_ms="500"/>
    <Retreat distance="0.2" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

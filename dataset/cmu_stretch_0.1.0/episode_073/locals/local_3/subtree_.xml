<BehaviorTree ID="MainTree">
  <Sequence>
    <DetectObject target="drawer_handle" timeout_ms="800"/>
    <MoveTo target="drawer_handle" timeout_ms="1200"/>
    <RetryUntilSuccessful num_attempts="3">
      <ApproachAndAlign target="drawer_handle" tolerance="0.01" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <IsAt target="drawer_handle"/>
  </Sequence>
</BehaviorTree>

<BehaviorTree ID="MainTree">
  <Sequence>
    <RetryUntilSuccessful num_attempts="2">
      <Action ID="DetectObject" target="green_bin_lid" timeout_ms="1200"/>
    </RetryUntilSuccessful>
    <Action ID="ApproachAndAlign" target="green_bin_lid" tolerance="0.01" timeout_ms="1500"/>
    <Action ID="MoveAbove" target="green_bin_lid" offset_z="0.05" timeout_ms="1200"/>
    <Condition ID="IsAt" target="green_bin_lid"/>
  </Sequence>
</BehaviorTree>

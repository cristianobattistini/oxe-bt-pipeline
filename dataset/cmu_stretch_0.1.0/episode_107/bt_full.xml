<BehaviorTree ID="MainTree">
  <Sequence>
    <Fallback>
      <DetectObject target="dishwasher_handle" timeout_ms="1200"/>
      <ScanForTarget target="dishwasher_handle" pattern="raster" max_attempts="3" timeout_ms="1500"/>
    </Fallback>
    <MoveTo target="dishwasher_front" timeout_ms="1500"/>
    <OpenGripper width="0.09" timeout_ms="800"/>
    <SetTCPYaw yaw_deg="90"/>
    <ApproachAndAlign target="dishwasher_handle" tolerance="0.005" timeout_ms="1500"/>
    <CloseGripper force="20" timeout_ms="1200"/>
    <OpenContainer target="dishwasher_door" container_type="hinged_door" timeout_ms="1500"/>
    <ContainerOpen target="dishwasher_door"/>
    <Retreat distance="0.1" timeout_ms="1200"/>
    <OpenGripper width="0.09" timeout_ms="800"/>
  </Sequence>
</BehaviorTree>

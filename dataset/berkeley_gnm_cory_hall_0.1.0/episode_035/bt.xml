<BehaviorTree ID="MainTree">
  <Sequence>
    <SetBlackboard key="nav_target" value="goal"/>
    <Fallback>
      <IsAt target="goal"/>
      <RetryUntilSuccessful num_attempts="3">
        <NavigateToNamed location_id="goal" yaw_deg="0" timeout_ms="5000"/>
      </RetryUntilSuccessful>
    </Fallback>
  </Sequence>
</BehaviorTree>

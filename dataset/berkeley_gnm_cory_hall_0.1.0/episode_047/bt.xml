<BehaviorTree ID="MainTree">
  <Fallback>
    <Condition ID="IsAt" target="goal"/>
    <Sequence>
      <RetryUntilSuccessful num_attempts="3">
        <Timeout timeout_ms="7000">
          <Action ID="NavigateToNamed" location_id="goal" yaw_deg="0" timeout_ms="5000"/>
        </Timeout>
      </RetryUntilSuccessful>
      <Condition ID="IsAt" target="goal"/>
    </Sequence>
  </Fallback>
</BehaviorTree>
